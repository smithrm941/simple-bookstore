<%- include header.ejs %>

</div>
  <div class = "all-books">
    <table class="table table-hover table-responsive">
      <thead>
        <th>Title</th>
        <th>Author</th>
        <th>Genre</th>
      </thead>
      <tbody>
        <% searchResult.forEach(function(book){ %>
          <tr>
            <td>
              <a href="/bookstore/<%= book.id %>">
                <%= book.title %>
              </a>
            </td>
            <td><%= book.author %></td>
            <td><%= book.genre %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
<%- include footer.ejs %>
